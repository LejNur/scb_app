{
  "version": 2,
  "builds": [
    {
      "app": "app/page.tsx",
      "use": " @vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/page.tsx",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "header": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
