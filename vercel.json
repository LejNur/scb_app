{
  "version": 2,
  "builds": [
    {
      "app": "package.json",
      "use": " @vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "header": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
